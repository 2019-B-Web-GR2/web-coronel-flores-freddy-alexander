<h1>Tabla mostrar usuario</h1>





<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Nombre</th>
        <th scope="col">Cedula</th>
        <th scope="col">Acciones</th>
    </tr>
    </thead>
    <tbody>
    <% users.forEach( (user) => {%>
    <tr>
        <th scope="row"><%= user.id %></th>
        <td><%= user.nombre %></td> <!-- table detail -->
        <td> <%= user.cedula %> </td>
        <td>
            <button
                    type="button"
                    class="btn btn-info">
                Editar
            </button>

            <form action="/usuario/<%= user.id %>"
                  method="POST">
                <button
                        type="submit"
                        class="btn btn-danger" >
                    Eliminar
                </button>

            </form>
        </td>
    </tr>
    <%});%>
    </tbody>
</table>
